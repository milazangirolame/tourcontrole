<table class='table-format reg-fonts'>
  <tr>
    <th>Nome</th>
    <th>Sobrenome</th>
    <th>Email</th>
    <th>Telefone</th>
    <th></th>
  </tr>
  <% @order.guests.each do |guest|  %>
    <tr>
      <td> <%= guest.first_name  %></td>
      <td> <%= guest.last_name  %></td>
      <td> <%= guest.email  %></td>
      <td> <%= guest.phone  %></td>
      <td> <%= buyer_icon(guest) %></td>
    </tr>
  <% end %>

</table>
